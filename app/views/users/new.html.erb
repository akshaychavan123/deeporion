<h1> Creating New User </h1>

<%= form_with model: @user do |form| %>
   <div> <%= form.label :name %>
    <%= form.text_field :name %> </div> <br>

    <div> <%= form.label :task %>
    <%= form.text_field :task %> </div><br>

    <div> <%= form.label :start_time %>
    <%= form.time_field :start_time %> </div><br>

    <div> <%= form.label :end_time %>
    <%= form.time_field :end_time %> </div><br>

    <%= form.submit %>

    <% end %>

